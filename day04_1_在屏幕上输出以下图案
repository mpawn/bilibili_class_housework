// 在屏幕上输出以下图案：
//       *
//      ***
//     *****
//    *******
//   *********
//  ***********
// *************
//  ***********
//   *********
//    *******
//     *****
//      ***
//       *
#include<stdio.h>
int main()
{
printf("      *\n");
printf("     ***\n");
printf("    *****\n");
printf("   *******\n");
printf("  *********\n");
printf(" ***********\n");
printf("*************\n");
printf(" ***********\n");
printf("  *********\n");
printf("   *******\n");
printf("    *****\n");
printf("     ***\n");
printf("      *\n");
    return 0;
}
