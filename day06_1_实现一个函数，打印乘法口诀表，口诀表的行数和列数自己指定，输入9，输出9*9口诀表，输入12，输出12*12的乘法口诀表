//实现一个函数，打印乘法口诀表，口诀表的行数和列数自己指定，
//输入9，输出9*9口诀表，输入12，输出12*12的乘法口诀表。
#include<stdio.h>

int box(int x)
{
    int i,j;
    for(i=1;i<=x;i++)
    {
        for(j=1;j<=x;j++)
        {
            printf("%d * %d\n",i,j);
        }
    }   
}

int main()
{
    int input;
    printf("打印乘法口诀表，口诀表的行数和列数自己指定，请输入一个数：>");

    scanf("%d",&input);

    box(input);

    return 0;
}
