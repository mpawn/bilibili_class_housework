//使用函数实现两个数的交换
#include<stdio.h>

void jiaohuan(int* a, int* b)
{
    int* c=&c;
    *c=*a;
    *a=*b;
    *b=*c;
    //return语句不能直接返回多个值
}

int main()
{
    int a,b,c=0;
    printf("使用函数实现两个数的交换,请输入两个数");
    scanf("%d%d",&a,&b);
    jiaohuan(&a,&b);

    printf("%d  %d",a,b);
    return 0;
}

// 以类型指针方式传递函数参数
