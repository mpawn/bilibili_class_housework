//求10 个整数中最大值
#include<stdio.h>
int main()
{   
    int max=0;
    int arr[10]={0};
    int i,input,last,now,output;
    printf("求10 个整数中最大值:\n");

    scanf("%d",&input);
    last=input;

    for(i=0;i<9;i++)
    {
      scanf("%d",&input);
      now=input;
      if(now>last)
      last=now;
    }
    printf("10 个整数中最大值为%d",last);

    return 0;
}
