//将三个数按从大到小输出
#include <stdio.h>
int main()
{
    int a,b,c;
    printf("请输入三个数，我将将三个数按从大到小输出：");
    scanf("%d%d%d",&a,&b,&c);
    if(a>=b&&a>=c)
        {
        if(b>=c)
            printf("%d %d %d",a,b,c);
        else
            printf("%d %d %d",a,c,b);
        }
    else if(c>=a&&c>=b)
        {
            {
            if(a>=b)
                printf("%d %d %d",c,a,b);
            else
                printf("%d %d %d",c,b,a); 
            }

        }
    else 
        {
            {
            if(a>=c)
                printf("%d %d %d",b,a,c);
            else
                printf("%d %d %d",b,c,a); 
            }

        }
    return 0;
}
